    "Y_err =data_scal['ESS_cps_norm_REF_and_dose_err'].values\n",
    "Y_fit = get_esf_fit(X, Y)\n",
    "X_fit = np.linspace(-7, 7, 1000)\n",
    "color = 'darkgreen'\n",
