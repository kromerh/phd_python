    "ax3.errorbar(X, Y, xerr=None, yerr=Y_err, ls='none', color=color, capsize=5)\n",
    "leg3 = ax3.legend(loc=\"best\",  fontsize=fs-4)\n",
    "\n",
    "# one liner to remove *all axes in all subplots*\n",
    "# plt.setp(plt.gcf().get_axes(), xticks=[], yticks=[]);\n",
