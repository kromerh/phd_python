    "Y_err =data_scal['ESS_cps_norm_dose_err'].values\n",
    "color = 'darkred'\n",
    "lbl = 'normed dose'\n",
    "# ax2 = ax1.twinx()\n",
    "ax2.plot(X, Y, marker='o', color=color, label=lbl)\n",
    "ax2.errorbar(X, Y, xerr=None, yerr=Y_err, ls='none', color=color, capsize=5)\n",
